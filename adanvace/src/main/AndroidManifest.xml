<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adanvace">


    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />


    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"></uses-permission>

    <!--允许联网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--获取GSM（2g）、WCDMA（联通3g）等网络状态的信息  -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--获取wifi网络状态的信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--保持CPU 运转，屏幕和键盘灯有可能是关闭的,用于文件上传和下载 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!--获取sd卡写的权限，用于文件上传和下载-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--允许读取手机状态 用于创建BmobInstallation-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".application.MyApp"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.view.VerticalActivity" />

        <activity android:name=".activity.view.CustomActivity" />

        <activity android:name=".activity.view.RadiusActivity" />

        <activity android:name=".activity.view.SoundActivity" />

        <activity android:name=".activity.view.VolleyActivity" />

        <activity android:name=".activity.view.ContactActivity" />

        <activity android:name=".activity.view.ViewPagerActivity" />

        <activity android:name=".activity.viewpager.TestViewPageOneActivity" />

        <activity android:name=".activity.viewpager.TestViewPageTwoActivity" />

        <activity android:name=".activity.viewpager.TestViewPagerThreeActivity" />

        <activity android:name=".activity.view.RefreshListViewActivity" />

        <activity android:name=".activity.view.ScrollViewActivity" />

        <activity android:name=".activity.viewgroup.ViewGroupActivity" />

        <activity android:name=".activity.viewgroup.FlowViewGroupActivity" />

        <activity android:name=".activity.viewgroup.AdapterActivity" />

        <activity android:name=".activity.viewgroup.RecyclerActivity" />

        <activity android:name=".activity.view.LotteryActivity" />

        <activity android:name=".activity.animation.AnimationActivity" />

        <activity android:name=".activity.animation.AnimatorActivity" />

        <activity android:name=".activity.viewgroup.HorizontalScrollViewActivity" />

        <activity android:name=".activity.viewgroup.JigsawActivity"/>
    </application>

</manifest>