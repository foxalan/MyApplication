<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sportlogs">

    <application
        android:name=".application.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/sport"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sport"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.HomeActivity" />

        <activity
            android:name=".activity.JournalFillActivity"
            android:label="添加日志" />

        <activity android:name=".activity.TargetFillActivity"
            android:label="添加目标"/>
        <activity android:name=".activity.ModifyTargetActivity"
            android:label="修改目标"/>


    </application>

</manifest>